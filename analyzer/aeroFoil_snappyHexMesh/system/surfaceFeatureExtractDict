FoamFile
{
    version         2.4;
    format          ascii;
    class           dictionary;
    object          surfaceFeatureExtractDict;
    location        "system";
}

geom.stl
{
    extractionMethod extractFromSurface;
    writeObj        yes;
    
    extractFromSurfaceCoeffs
    {
        includedAngle   150;
    }
}


cylinder.stl
{
    extractionMethod extractFromSurface;
    writeObj        yes;
    
    extractFromSurfaceCoeffs
    {
        includedAngle   150;
    }
}



