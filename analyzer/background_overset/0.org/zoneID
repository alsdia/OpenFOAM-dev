FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      zoneID;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    frontAndBack2
    {
        type            symmetry;
    }
    outerWalls
    {
        type            zeroGradient;
    }
    frontAndBack1
    {
        type            symmetry;
    }
    walls
    {
        type            zeroGradient;
    }
    free
    {
        type            overset;
    }
}


